{
  "name": "node-modules-inspector",
  "type": "module",
  "version": "0.2.9",
  "description": "A Node Modules Inspector",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/node-modules-inspector#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/node-modules-inspector.git"
  },
  "bugs": "https://github.com/antfu/node-modules-inspector/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "dev": "npm run stub --workspaces && cd src && nuxi dev",
    "stub": "unbuild --stub",
    "build": "npm run wc:prepare && nuxi build src && unbuild",
    "build:debug": "NUXT_DEBUG_BUILD=true npm run build",
    "start": "node ./bin.mjs",
    "prepack": "npm build",
    "dev:prepare": "nuxi prepare src && npm run wc:prepare",
    "wc:prepare": "rollup -c",
    "wc:build": "npm run wc:prepare && NMI_BACKEND=webcontainer nuxi build src && unbuild",
    "wc:dev": "npm run --workspaces stub && npm run wc:prepare && cd src && NMI_BACKEND=webcontainer nuxi dev"
  },
  "dependencies": {
    "ansis": "latest",
    "birpc": "latest",
    "cac": "latest",
    "debug": "latest",
    "fast-glob": "latest",
    "fast-npm-meta": "latest",
    "flatted": "latest",
    "get-port-please": "latest",
    "h3": "latest",
    "launch-editor": "latest",
    "mlly": "latest",
    "mrmime": "latest",
    "node-modules-tools": "../node-modules-tools",
    "ohash": "latest",
    "open": "latest",
    "pathe": "latest",
    "publint": "latest",
    "structured-clone-es": "latest",
    "unconfig": "latest",
    "unstorage": "latest",
    "ws": "latest"
  },
  "devDependencies": {
    "@types/picomatch": "latest",
    "@types/semver": "latest",
    "@unocss/nuxt": "latest",
    "@vueuse/nuxt": "latest",
    "@webcontainer/api": "latest",
    "@xterm/addon-fit": "latest",
    "@xterm/xterm": "latest",
    "d3": "latest",
    "d3-hierarchy": "latest",
    "d3-shape": "latest",
    "floating-vue": "latest",
    "fuse.js": "latest",
    "idb-keyval": "latest",
    "modern-screenshot": "latest",
    "picomatch": "latest",
    "semver": "latest",
    "shiki": "latest",
    "theme-vitesse": "latest",
    "vite-hot-client": "latest"
  }
}
